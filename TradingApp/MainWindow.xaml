<Window x:Class="TradingApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FinamBot" Height="400" Width="700">
    <Grid Margin="10">
        <Button Name="SendButton" Content="Получить деньги"
                HorizontalAlignment="Center" VerticalAlignment="Center"
                Padding="10,5" Click="SendButton_Click"/>
        <Button Content="Получить стакан SBER"
                Command="{Binding ShowOrderBookCommand}"
                Margin="0 100 0 0" Height="30" Width="150"/>
    </Grid>
</Window>
